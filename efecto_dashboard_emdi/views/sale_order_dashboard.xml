<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id ="sale_order_dashboard_tree" model="ir.ui.view">
        <field name="name">sale.dashboard.tree</field>
        <field name="model">sale.dashboard</field>
        <field name="type">tree</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name" />
            </tree>
        </field>
    </record>

    <record id ="sale_order_dashboard_form" model="ir.ui.view">
        <field name="name">sale.dashboard.form</field>
        <field name="model">sale.dashboard</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <from>
                <field name="name" />
            </from>
        </field>
    </record>

</odoo>